    "@vueuse/integrations": "^12.8.2",
    "animate.css": "^4.1.1",
    "await-to-js": "^3.0.0",
    "axios": "^1.8.4",
    "bignumber.js": "^9.2.1",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "element-plus": "^2.9.7",
    "fuse.js": "^7.1.0",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.3.1",
    "qs": "^6.14.0",
    "socket.io-client": "^4.8.1",
    "tinymce": "^7.8.0",
    "universal-cookie": "^7.2.2",
    "vue": "^3.5.13",
    "vue-hooks-plus": "2.2.3",
    "vue-i18n": "^11.1.3",
    "vue-router": "^4.5.0",
    "vxe-pc-ui": "^4.5.22",
    "vxe-table": "^4.13.2"
  }
