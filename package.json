{
  "name": "vue-admin-template",
  "version": "0.0.1",
  "type": "module",
  "description": "vue3后台项目模板",
  "scripts": {
    "dev": "vite --mode dev",
    "uat": "vite --mode uat",
    "pro": "vite --mode pro",
    "ts:check": "vue-tsc --noEmit",
    "eslint": "eslint --config eslint.config.ts ./src/views/order-center/sales-order/SalesOrderList.vue",
    "lint:eslint": "eslint --config eslint.config.ts --fix ./src",
    "lint:staged": "lint-staged",
    "build:pro": "vue-tsc --noEmit && vite build --mode pro",
    "build:uat": "vue-tsc --noEmit && vite build --mode uat",
    "build:dev": "vue-tsc --noEmit && vite build --mode dev",
    "prepare": "husky"
  },
  "keywords": [
    "vue-admin"
  ],
  "author": "MMT FE",
  "license": "ISC",
  "lint-staged": {
    "*.{js,ts,vue,jsx,tsx}": [
      "eslint --config eslint.config.ts ./src"
    ]
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@higgins-mmt/vite-plugin-i18n-transformer": "^1.2.0",
    "@iconify/vue": "^4.3.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.1",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.18",
    "@unocss/eslint-plugin": "^65.5.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.24.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^15.5.1",
    "sass": "^1.86.3",
    "typescript": "^5.8.3",
    "unocss": "^65.5.0",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.6",
    "vue-tsc": "^2.2.8"
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "^6.1.0",
    "@visactor/vtable": "^1.17.6",
